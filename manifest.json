{
  "name": "zukan",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Bookmark web pages to Gyazo. ",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "permissions": ["bookmarks"]
}
