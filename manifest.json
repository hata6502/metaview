{
  "name": "metaview",
  "version": "2.2.1",
  "manifest_version": 3,
  "description": "Bookmark web pages to Scrapbox. ",
  "action": {},
  "background": {
    "service_worker": "dist/background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+Shift+M"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "options_page": "options.html",
  "permissions": ["storage"]
}
